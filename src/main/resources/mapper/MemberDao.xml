<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.xinqushi.wechat.web.dao.MemberDao">
<select id="getByUserId" resultType="net.xinqushi.wechat.entity.Member">
  select id,uid,name,sex, mobile from member
    where uid=#{uid}
</select>


</mapper>